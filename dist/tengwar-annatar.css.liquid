
@font-face {
    font-family: Tengwar Annatar;
    src: url("{{'tengwar-annatar.eot' | asset_url }}");
    src: url("{{'tengwar-annatar.eot#iefix' | asset_url | replace: '%23','#' }}"),
         url("{{'tengwar-annatar.woff' | asset_url }}") format('woff'),
         url("{{'tengwar-annatar.ttf' | asset_url }}") format('truetype'),
         url("{{'tengwar-annatar.svg#TengwarAnnatarItalic' | asset_url | replace: '%23','#' }}") format('svg');
    font-weight: normal;
    font-style: normal;
}

.tengwar.annatar.rendered {
    font-family: Tengwar Annatar;
    font-size: 30px;
    line-height: 240%;
}

